{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "pgehandler",
            "image": "mblevins/homepower-pgehandler:${project.version}-${build.id}",
            "environment": [
                {
                    "name": "mykey",
                    "value": "myvalue"
                }
            ],
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 8080
                }
            ],
            "essential": true,
            "memory": 300
        }
    ]
}